{
  "name": "apie/serializer",
  "description": "Composer package of the apie library: serializer",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Serializer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Serializer\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\Serializer\\Lists\\SerializedHashmap",
      "Apie\\Serializer\\Lists\\SerializedList"
    ]
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "apie/type-converter": "^1.3.1",
    "psr/http-message": "^1.1",
    "symfony/http-foundation": "^7.2"
  },
  "require-dev": {
    "apie/common-value-objects": "self.version",
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.3",
    "apie/text-value-objects": "self.version",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}