{
  "name": "apie/serializer",
  "description": "Composer package of the apie library: serializer",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Serializer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Serializer\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "apie/type-converter": "^1.0.5",
    "psr/http-message": "^1.0",
    "symfony/http-foundation": "^6.1"
  },
  "require-dev": {
    "apie/common-value-objects": "self.version",
    "apie/fixtures": "self.version",
    "apie/text-value-objects": "self.version",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}